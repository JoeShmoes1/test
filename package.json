{
  "name": "goal-planner",
  "version": "1.0.0",
  "description": "An application that allows users to input their goals and generates tailored questions to help create a personalized plan for achieving those goals.",
  "main": "src/electron/main.js",
  "scripts": {
    "start": "vite",
    "dev": "concurrently \"npm run start:react\" \"npm run start:electron\"",
    "dev:full": "concurrently \"npm run start:react\" \"npm run start:backend\" \"npm run start:electron\"",
    "start:react": "vite",
    "start:backend": "cd backend && python app.py",
    "start:electron": "wait-on tcp:3000 && cross-env NODE_ENV=development electron .",
    "build": "tsc && vite build",
    "test": "jest",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "crypto-js": "^4.2.0",
    "daisyui": "^5.0.43",
    "electron-squirrel-startup": "^1.0.0",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.513.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.6.2",
    "tailwindcss": "^3.4.17",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^27.0.0",
    "@types/node": "^16.18.126",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.5.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.1.0",
    "cross-env": "^7.0.3",
    "electron": "^25.0.1",
    "postcss": "^8.5.4",
    "typescript": "^5.8.3",
    "vite": "^4.5.14",
    "wait-on": "^7.0.1"
  },
  "author": "",
  "license": "ISC"
}
